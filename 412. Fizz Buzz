var fizzBuzz = function(n) {
    var aResult = [];
    for(var i=1; i<=n; i++){
        if(i%15 == 0){
            aResult.push("FizzBuzz");
            continue;
        }
        if(i%3 == 0){        
            aResult.push("Fizz");
            continue;
        }
        if(i%5 == 0){      
            aResult.push("Buzz");
            continue;
        }
        aResult.push((i+""));
    }
    return aResult;
};
